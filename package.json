{
	"name": "@gin-quin/minimix",
	"module": "index.ts",
	"type": "module",
	"version": "1.0.1",
	"scripts": {
		"build": "bun build library/*.ts --minify --sourcemap --outdir library",
		"test": "bun test",
		"deploy": "bun run build && bun run test && bun publish --access=public --allow-dirty && bun x jsr publish --allow-dirty"
