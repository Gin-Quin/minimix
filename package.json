		".": "./library/index.js",
		"./*": "./library/*.js"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/bun": "latest",
		"gzip-size": "^7.0.0",
		"ts-mixer": "^6.0.4"
	}
}
